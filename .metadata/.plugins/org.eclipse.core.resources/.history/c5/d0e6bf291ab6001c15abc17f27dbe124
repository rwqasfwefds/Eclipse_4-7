package codeup;

import java.util.Scanner;

public class codeup {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scanner = new Scanner(System.in);

		String x[] = scanner.nextLine().split(".");
		

		System.out.printf("%d.%02d.%02d", x[0], x[1], x[2]);
	}

}
