package ex;

public class Ex3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
//		Ex3_1
//		int x, y;
//		
//		x = y = 3;
//		System.out.println("x = " + x);
//		System.out.println("y = " + y);
		
		int i = 5, j = 0;
		
		j = i++;
		System.out.println("j = i++; 실행 후, i = " + i + ", j = " + j);
		
		i = 5;
		j = 0;
		
		j = ++i;
		System.out.println("j = ++i; 실행 후, i = " + i + ", j = " + j);
	}

}
